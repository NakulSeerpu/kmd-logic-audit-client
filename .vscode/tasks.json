{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "build",
        "command": "dotnet",
        "type": "process",
        "args": [
          "build",
          "${workspaceFolder}",
          "/p:GenerateFullPaths=true"
        ],
        "problemMatcher": "$msCompile",
        "group": {
          "kind": "build",
          "isDefault": true
        }
      },
      {
        "label": "rebuild",
        "command": "dotnet",
        "type": "process",
        "args": [
          "build",
          "/t:rebuild",
          "${workspaceFolder}",
          "/p:GenerateFullPaths=true"
        ],
        "problemMatcher": "$msCompile",
        "group": "build"
      }
    ]
}